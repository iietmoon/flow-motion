function logError(e,o){console.error("An error occurred:",o)}function createErrorResponse(e,o){return{message:e,error:o}}function applyAnimation(e,o,n,t,r,i){e.style[o]=n,e.style.transition="".concat(r,"ms ").concat(i," ").concat(o),setTimeout(function(){e.style[o]=t},r)}function revertAnimation(e,o,n,t,r,i){e.style[o]=t,e.style.transition="".concat(r,"ms ").concat(i," ").concat(o),setTimeout(function(){e.style[o]=n},r)}function animateElements(e){var o,n=e.element,t=e.properties,i=e.duration,a=e.easing,e=e.trigger,n=document.querySelectorAll(n);if(0===n.length)return logError(o="No elements found for the provided selector."),createErrorResponse(o);"hover"===e&&n.forEach(function(r){t.forEach(function(e){var o=e.property,n=e.from,t=e.to,e=e.delay;setTimeout(function(){r.addEventListener("mouseover",function(e){applyAnimation(r,o,n,t,i,a)}),r.addEventListener("mouseleave",function(e){revertAnimation(r,o,n,t,i,a)})},void 0===e?0:e)})})}var FlowMotion={animate:animateElements};window.FlowMotion=FlowMotion;