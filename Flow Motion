function interpolate(){return null}function logError(e,o){console.error("An error occurred:",o)}function createErrorResponse(e,o){return{message:e,error:o}}function applyAnimationStyles(e,o,r,n,t,i){e.style[o]=r,e.style.transition="".concat(t,"ms"),setTimeout(function(){e.style[o]=n},t)}function animateElements(e){var o=e.element,r=e.properties,i=e.duration,l=e.easing,e=(e.trigger,document.querySelectorAll(o));if(0===e.length)return logError(o="No elements found for the provided selector."),createErrorResponse(o);e.forEach(function(t){r.forEach(function(e){var o=e.property,r=e.from,n=e.to,e=e.delay;setTimeout(function(){applyAnimationStyles(t,o,r,n,i,l)},void 0===e?0:e)})})}var FlowMotion={animate:animateElements};window.FlowMotion=FlowMotion;