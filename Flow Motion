function interpolate(){return null}function logError(e,r){console.error("An error occurred:",r)}function createErrorResponse(e,r){return{message:e,error:r}}function applyAnimationStyles(e,r,n,o,t,i){e.style[r]=n,e.style.transition="".concat(t,"ms"),setTimeout(function(){e.style[r]=o},t)}function animateElements(e){var r=e.element,n=e.properties,t=e.duration,i=e.easing,e=(e.trigger,document.querySelectorAll(r));if(0===e.length)return logError(r="No elements found for the provided selector."),createErrorResponse(r);e.forEach(function(o){n.forEach(function(e){var r=e.property,n=e.from,e=e.to;applyAnimationStyles(o,r,n,e,t,i)})})}var FlowMotion={animate:animateElements};